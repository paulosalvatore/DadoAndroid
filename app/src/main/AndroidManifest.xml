<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="paulosalvatore.com.br.dadoandroid">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">

		<activity
			android:name="paulosalvatore.com.br.dadoandroid.SplashScreen"
			android:theme="@style/FullScreen">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>

		<activity android:name="paulosalvatore.com.br.dadoandroid.PlayerActivity">
		</activity>

		<activity android:name="paulosalvatore.com.br.dadoandroid.GameActivity">
		</activity>

	</application>

</manifest>
